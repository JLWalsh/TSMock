{
  "name": "tsmock",
  "version": "1.0.0",
  "description": "A library that facilitates mocking classes & functions in Typescript.",
  "repository": "git@github.com:JLWalsh/TSMock.git",
  "author": "James L. Walsh",
  "license": "MIT",
  "dependencies": {
    "reflect-metadata": "^0.1.13",
    "ts-node": "^8.0.3",
    "typescript": "^3.3.3"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsc"
  },
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "@types/node": "^10.12.22",
    "@types/reflect-metadata": "^0.1.0",
    "ts-jest": "^23.10.5"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "/__tests__/.*\\.(ts)$",
    "roots": [
      "<rootDir>/src"
    ]
  }
}
